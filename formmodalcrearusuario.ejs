
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>SoliPrEs</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->  
  <link rel="shortcut icon" href="img/icons/icon-48x48.png" />
  <link href="../img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="../vendor/aos/aos.css" rel="stylesheet">
  <link href="../css/bootstrap.min.css" rel="stylesheet">
  <link href="../vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="../vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="../vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
  <link href="../vendor/remixicon/remixicon.css" rel="stylesheet">
  <link href="../vendor/swiper/swiper-bundle.min.css" rel="stylesheet">
  <link href="../css/app.css" rel="stylesheet">
  <link href="../css/dataTables.bootstrap4.min.css" rel="stylesheet">
  <link rel="stylesheet"  href="../css/dataTables.jqueryui.css"/>		

  <!-- Template Main CSS File -->
  <link href="../css/style.css" rel="stylesheet">
 

</head>

<body>

<!--Formulario Modal-->
  
<!-- Agregar usuario -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h3 class="modal-title" id="exampleModalLabel">Agregar Usuario</h3>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <form action="/save" method="POST" class="needs-validation my-4">
           <div class="col">	
             <label for="validationCustom01" class="form-label">Nombre</label>				 
             <input type="text" class="form-control" id="validationCustom01" name="nombre" tabindex="4" required>
           </div>
           <div class="col">  
             <label for="validationCustom01" class="form-label">1er Apellido</label>                   
              <input type="text" class="form-control" id="validationCustom01" name="primer_apellido" tabindex="5" required>
           </div>
           <div class="col"> 
             <label for="validationCustom01" class="form-label">2do Apellido</label> 					
              <input type="text" class="form-control" id="validationCustom01" name="segundo_apellido" tabindex="6" required>
           </div>
           <div class="col">  
             <label for="validationDefaultUsername" class="form-label">Usuario</label>
              <input type="text" class="form-control" id="validationDefaultUsername" name="usuario" tabindex="2"  required>
            </div>
            <div class="col">
             <label for="validationCustom01" class="form-label">Contraseña</label>
              <input type="password" class="form-control" id="validationCustom01" name="contrasena" tabindex="3" required>					
           </div>
         <div class="col my-2">
            <label for="validationCustom04" class="form-label" >Cargo</label>
            <select name="cargo" id="validationCustom04" class="form-select" tabindex="7" required>
           <option selected disabled value="">...</option>
             <option value="Informático">Informático</option>
             <option value="Logístico">Logístico</option>
             <option value="DirEconómico">Dir. Económico</option>
             <option value="EconómicoUEB">Económico UEB</option>
             <option value="DirectorEMP">Director Emp</option>
             <option value="DirUEB">Dir. UEB</option>
            </select>
          </div>				   
          <div class="col my-2">
         <label for="validationCustom04" class="form-label">Rol</label>
            <select name="rol" id="validationCustom04" class="form-select" tabindex="8" required>
           <option selected disabled value="">...</option>
             <option value="Administrador">Administrador</option>
             <option value="Logístico">Logístico</option>
             <option value="DirEconómico">Dir. Económico</option>
             <option value="EconómicoUEB">Económico UEB</option>
             <option value="DirectorEMP">Director Emp</option>
             <option value="DirUEB">Dir. UEB</option>
            </select>
         </div>	
         <div class="col">  
           <label for="validationCustom01" class="form-label">CI</label>
           <input type="text" class="form-control" id="validationCustom01" name="CI" tabindex="1"  required>
          </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="submit" class="btn btn-outline-secondary my-4" tabindex="11">Agregar</button>
      </div>
      </div>
    </form>
  </div>
</div>  
</div> 
</div>
</div>
 <!--Culmina Formulario Modal Agregar Usuario-->   
<!-- Vendor JS Files -->
  <script src="../vendor/aos/aos.js"></script>
  <script src="../js/bootstrap.bundle.min.js"></script>
  <script src="../vendor/glightbox/js/glightbox.min.js"></script>
  <script src="../vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="../vendor/php-email-form/validate.js"></script>
  <script src="../vendor/purecounter/purecounter.js"></script>
  <script src="../vendor/swiper/swiper-bundle.min.js"></script>
  <script src="../js/app.js"></script>  
  <script src="../js/jquery.js"></script> 	
  <script  src="../js/jquery.dataTables.min.js"></script>   
  <script  src="../js/dataTables.bootstrap4.min.js"></script> 
  <script  src="../js/dataTables.jqueryui.js"></script> 
  <script  src="../js/dataTables.responsive.min.js"></script> 
  <script src="../js/sweetalert2/dist/sweetalert2.all.min.js"></script>
	 
	<!--Config Alertas-->
  <% if(typeof alert != "undefined") { %>
    <script>
       Swal.fire({
        title: '<%= alertTitle %>',
        text:'<%= alertMessage %>',  
        icon: '<%= alertIcon %>',
        showConfirmButton: '<%= showConfirmButton %>',
        timer: '<%= timer %>'
       }).then(()=>{
         window.location='/<%= ruta %>'
       })
    </script>
    <% } %>

  <script>$('#stilotableusuarios').DataTable({   
		responsive:true,
		autowidth:false,
	});</script>
  
  <!-- Template Main JS File -->
  <script src="../js/main.js"></script>

</body>
</html>